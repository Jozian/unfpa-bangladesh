<div class="panel">
  <form id="index_filter_form" autocomplete="off">
    <% if filters_to_show.include? 'Age Range'%>
        <%= build_filter_checkboxes(t("potential_matches.filter_by.age_range_case"), "child_age", [
            '0-5',
            '6-11',
            '12-17',
            '18+'
        ], nil, nil, 'range') %>
    <% end %>
    <% if filters_to_show.include? 'Sex'%>
        <%= build_filter_checkboxes(t("potential_matches.filter_by.sex_case"), "child_sex",
                                    Lookup.values('lookup-gender', @lookups, locale: I18n.locale)) %>
    <% end %>
  </form>
  <div class="clearfix"></div>
</div>
