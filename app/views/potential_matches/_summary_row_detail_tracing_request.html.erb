<tr>
  <td class="id_column">
    <% if detail.visible %>
      <%= link_to text_to_identify_potential_match_tracing_request(detail), tracing_request_path(detail.tracing_request_id), class: 'id_link ' %>
    <% else %>
      <%= detail.tracing_request_id.last 7 %>
    <% end %>
  </td>
  <td><%= detail.tracing_request_relation_name %></td>
  <td><%= field_format_date(detail.tracing_request_inquiry_date) %></td>
  <td><%= detail.tracing_request_name %></td>
  <td><%= detail.tracing_request_owned_by %></td>
  <td><%= detail.average_rating.round(5) %></td>
</tr>