<% display_alert_approval = display_approval_alert(formObject, section) %>
<% if display_alert_approval %>
  <%= render 'form_section/header_message_approval', form_name: display_alert_approval, alert_type: 'approval' %>
<% end %>
<% display_alert_field_change = display_field_change_alert(formObject, section) %>
<% if display_alert_field_change %>
  <%= render 'form_section/header_message_approval', form_name: display_alert_field_change[:name],
             alert_type: 'field_change', form_date: display_alert_field_change[:date] %>
<% end %>
<% display_alert_transfer_request = display_transfer_request_alert(formObject, section) %>
<% if display_alert_transfer_request %>
  <%= render 'form_section/header_message_approval', form_name: section.unique_id, alert_type: 'transfer_request',
             form_date: display_alert_transfer_request[:date], form_user: display_alert_transfer_request[:user],
             form_agency: display_alert_transfer_request[:agency] %>
<% end %>